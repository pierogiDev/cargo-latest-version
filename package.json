{
  "name": "cargo-latest-version",
  "displayName": "Cargo Latest Version",
  "description": "Display latest version of crates in Cargo.toml",
  "version": "0.6.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/cargo-latest-version"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:toml",
    "workspaceContains:**/Cargo.toml",
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "toml",
        "extensions": [
          ".toml"
        ],
        "filenames": [
          "Cargo.toml"
        ]
      }
    ],
    "commands": [
      {
        "command": "cargo-latest-version.showLatestVersions",
        "title": "Show Latest Cargo Versions"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run esbuild-base -- --minify",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "test-compile": "tsc -p ./",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.10.0",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vscode/vsce": "^3.2.1",
    "esbuild": "^0.24.2",
    "eslint": "^8.54.0",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "axios": "^1.6.2"
  }
}
